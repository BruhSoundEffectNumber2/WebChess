    "clean-webpack-plugin": "3.0.0",
    "compression-webpack-plugin": "7.1.2",
    "css-loader": "^6.7.1",
    "eslint": "^8.15.0",
    "eslint-config-google": "^0.14.0",
    "html-webpack-plugin": "5.5.0",
    "sass": "^1.51.0",
    "sass-loader": "^12.6.0",
    "source-map-loader": "2.0.2",
    "style-loader": "^3.3.1",
    "terser-webpack-plugin": "5.3.1",
    "ts-loader": "9.2.8",
    "typescript": "4.6.3",
    "webpack": "5.72.0",
    "webpack-cli": "4.9.2",
    "webpack-dev-server": "4.8.1",
    "webpack-merge": "5.8.0"
